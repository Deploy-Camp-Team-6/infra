[DEFAULT]
bantime = 1h
findtime = 10m
maxretry = 5
backend = auto
usedns = warn
logencoding = auto
enabled = false
mode = normal
filter = %(__name__)s[mode=%(mode)s]

[sshd]
enabled = true
port = {{ ssh_port }}
logpath = %(sshd_log)s
backend = %(sshd_backend)s

[docker-auth]
enabled = true
filter = docker-auth
logpath = /var/log/auth.log
maxretry = 3
bantime = 1h